package Notification;

import LDraw.Support.LDrawDirective;

public class LDrawDirectiveRemoved implements INotificationMessage{
	private LDrawDirective parent;
	private int index;
	private LDrawDirective directive;
	
	public LDrawDirectiveRemoved(LDrawDirective parent, int index, LDrawDirective directive){
		this.parent = parent;
		this.index = index;
		this.directive = directive;
	}

	public LDrawDirective getParent() {
		return parent;
	}

	public int getIndex() {
		return index;
	}

	public LDrawDirective getDirective() {
		return directive;
	}
	
	
	
	
}
